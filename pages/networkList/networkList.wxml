<view class="networks-container">
  <view class="cell network-item" wx:for="{{ networks }}">
    <view class="cell-main">
      <text class="network-title">{{ item.name }}</text>
      <text>{{ item.address }}</text>
    </view>
    <block wx:if="{{ item.longitude && item.latitude }}">
      <image class="network-nav" src="/img/location.png" data-id="{{ item.id }}" bindtap="onOpenLocation"></image>
    </block>
  </view>
</view>