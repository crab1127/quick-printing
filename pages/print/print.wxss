.print-wrap{
    display: flex;
    flex-direction: column;
    height: 100%;
}
.print-code{
    flex: 2;
    display: flex;
    justify-content: center;
    align-items: center;
    position: relative;
    color: #1AAD19;
    background: #fff;
}
.print-code-text{
    position: absolute;
    width: 100%;
    text-align: center;
    color:#999;
    font-size: 28rpx;
    bottom: 30%;
    left:0;
}
.print-code-num{
    font-size: 60rpx;
}
.print-tips{
    flex:1;
    display: flex;
    justify-content: center;
    align-items: center;
    background:#eee;
}
.print-tips > .flex-col{
    flex: 1;
    padding: 30rpx;
}
.tips-img {
    width: 300rpx; 
    height:148.55rpx;
    margin-top: -80rpx;
    margin-bottom: 40rpx;
}