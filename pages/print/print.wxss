.print-wrap{
    display: flex;
    flex-direction: column;
    height: 100%;
}
.print-code{
    flex: 1;
    display: flex;
    justify-content: center;
    align-items: center;
    position: relative;
    color: #fff;
    background: #0c9;
}
.print-code-text{
    position: absolute;
    width: 100%;
    text-align: center;
    color:#eee;
    font-size: 28rpx;
    bottom: 30%;
    left:0;
}
.print-code-num{
    font-size: 60rpx;
}
.print-tips{
    flex:1;
    display: flex;
}
.print-tips > .flex-col{
    flex: 1;
    padding: 50rpx;
}
.tips-img {
    width: 276rpx; 
    height:120rpx;
    margin-top: -80rpx;
    margin-bottom: 40rpx;
}